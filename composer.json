{
    "name": "jad-network/laravel-solana",
    "description": "Solana Web3.js ported to PHP with Laravel",
    "keywords": [
        "JAD Network",
        "laravel",
        "laravel-solana"
    ],
    "homepage": "https://github.com/jad-network/laravel-solana",
    "license": "MIT",
    "authors": [
        {
            "name": "Dylan Marriott",
            "email": "dylan@onyxleeds.co.uk",
            "role": "Developer"
        }
    ],
    "require": {
        "php": "^8.3",
        "illuminate/console": "^10.0 || ^11.0",
        "illuminate/support": "^10.0 || ^11.0",
        "saloonphp/saloon": "^v3.0.0",
        "saloonphp/laravel-plugin": "^v3.0.0",
        "spatie/laravel-package-tools": "^1.16.6"
    },
    "require-dev": {
        "laravel/pint": "^1.14",
        "nunomaduro/collision": "^v8.0.0||^7.0.0",
        "larastan/larastan": "^2.9",
        "orchestra/testbench": "^9.0.0||^v8.0.0",
        "pestphp/pest": "^3.0",
        "pestphp/pest-plugin-arch": "^3.0",
        "pestphp/pest-plugin-laravel": "^3.0",
        "phpstan/extension-installer": "^1.3",
        "phpstan/phpstan-deprecation-rules": "^1.1",
        "phpstan/phpstan-phpunit": "^1.3"
    },
    "autoload": {
        "psr-4": {
            "JadNetwork\\LaravelSolana\\": "src/",
            "JadNetwork\\LaravelSolana\\Database\\Factories\\": "database/factories/"
        }
    },
    "autoload-dev": {
        "psr-4": {
            "JadNetwork\\LaravelSolana\\Tests\\": "tests/",
            "Workbench\\App\\": "workbench/app/"
        }
    },
    "scripts": {
        "post-autoload-dump": "@composer run prepare",
        "prepare": "@php vendor/bin/testbench package:discover --ansi",
        "analyse": "vendor/bin/phpstan analyse",
        "test": "vendor/bin/pest",
        "test-coverage": "vendor/bin/pest --coverage",
        "format": "vendor/bin/pint"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "pestphp/pest-plugin": true,
            "phpstan/extension-installer": true
        }
    },
    "extra": {
        "laravel": {
            "providers": [
                "JadNetwork\\LaravelSolana\\LaravelSolanaServiceProvider"
            ],
            "aliases": {
                "LaravelSolana": "JadNetwork\\LaravelSolana\\Facades\\LaravelSolana"
            }
        }
    },
    "minimum-stability": "dev",
    "prefer-stable": true
}
